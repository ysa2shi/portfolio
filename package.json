{
  "name": "portfolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "run-p --continue-on-error --print-label lint:*",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "lint:prettier": "prettier --cache --check .",
    "lint:secretlint": "secretlint --maskSecrets --secretlintignore .gitignore '**/*'",
    "fix": "run-s --continue-on-error fix:*",
    "fix:eslint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --fix",
    "fix:prettier": "prettier --cache --write ."
  },
  "dependencies": {
    "astro": "^5.7.13",
    "astro-google-fonts-optimizer": "^0.2.2",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.4.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.27.0",
    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tailwindcss": "3.4.17",
    "wrangler": "^4.16.1"
  }
}